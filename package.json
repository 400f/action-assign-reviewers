  "devDependencies": {
    "@types/node": "^14.14.9",
    "@typescript-eslint/parser": "^4.8.1",
    "@vercel/ncc": "^0.25.1",
    "eslint": "^7.17.0",
    "eslint-plugin-github": "^4.1.1",
    "eslint-plugin-jest": "^24.1.3",
    "js-yaml": "^3.14.0",
    "prettier": "2.2.1",
    "typescript": "^4.1.3"
